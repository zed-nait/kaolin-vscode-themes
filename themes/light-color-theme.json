  "tokenColors": [
    {
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#13665f"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#e36b3f"
      }
    },
    {
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#13665f"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#845a84"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#39854c"
      }
    },
    {
      "scope": [
        "punctuation.section.braces.begin"
      ],
      "settings": {
        "foreground": "#e36b3f"
      }
    },
    {
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#4c7a90"
      }
    },
    {
      "scope": [
        "comment.line",
        "comment.block"
      ],
      "settings": {
        "foreground": "#7c878a"
      }
    },
    {
      "scope": [
        "punctuation.decorator",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#cd5c60"
      }
    },
    {
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#cd5c60"
      }
    },
    {
      "scope": [
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#383e3f"
      }
    },
    {
      "scope": [
        "meta.diff.header.git",
        "meta.diff.index.git"
      ],
      "settings": {
        "foreground": "#73229e"
      }
    },
    {
      "scope": [
        "meta.diff.header.from-file",
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#cd5c60"
      }
    },
    {
      "scope": [
        "meta.diff.header.to-file",
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#40826d"
      }
    },
    {
      "scope": [
        "meta.diff.range.unified"
      ],
      "settings": {
        "foreground": "#91762a"
      }
    },
    {
      "scope": [
        "source.diff"
      ],
      "settings": {
        "foreground": "#383e3f"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#cd5c60"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#845a84"
      }
    },
    {
      "scope": [
        "meta.attribute.style"
      ],
      "settings": {
        "foreground": "#4c7a90"
      }
    }
  ]
